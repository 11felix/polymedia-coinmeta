{
    "private": false,
    "version": "0.0.18",
    "name": "@polymedia/coinmeta-react",
    "author": "@juzybits (https://polymedia.app)",
    "homepage": "https://github.com/juzybits/polymedia-coinmeta",
    "description": "It provides the React hooks useCoinMeta() and useCoinMetas()",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "https://github.com/juzybits/polymedia-coinmeta"
    },
    "keywords": [
        "polymedia",
        "sui",
        "coinmeta"
    ],
    "scripts": {
        "build": "tsc -p tsconfig.cjs.json && tsc -p tsconfig.esm.json",
        "clean": "rm -rf dist/ node_modules/ .turbo/",
        "dev": "tsc --watch -p tsconfig.esm.json",
        "lint": "eslint src/ --report-unused-disable-directives --max-warnings 0",
        "typecheck": "tsc -p tsconfig.esm.json"
    },
    "dependencies": {
        "@polymedia/coinmeta": "workspace:*"
    },
    "peerDependencies": {
        "@mysten/sui": "^1.15.1",
        "react": "18"
    },
    "devDependencies": {
        "@types/react": "^18.3.12",
        "typescript": "^5.7.2"
    },
    "type": "module",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist/"
    ],
    "types": "./dist/esm/index.d.ts",
    "module": "./dist/esm/index.js",
    "main": "./dist/cjs/index.js",
    "exports": {
        ".": {
            "import": "./dist/esm/index.js",
            "require": "./dist/cjs/index.js"
        }
    }
}
